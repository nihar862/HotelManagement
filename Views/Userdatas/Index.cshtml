@model IEnumerable<Hotel_Management.Userdata>

@{
    ViewBag.Title = "Index";
}

<h2>Registered User</h2>
<br/>
<br/>
<br/>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Username)
        </th>
        
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Username)
        </td>
        
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>

    </tr>
}

    
</table>


    
@Html.ActionLink("Manage Hotel", "Index", "Hotel_Data" ,null , new { @class = "btn btn-info", @style = "width:100 px" })
    

